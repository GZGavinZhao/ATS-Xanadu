########################################################################

testall:: ; make all > /dev/null

########################################################################

######
srclines:: ; wc -l \
HATS/*.hats SATS/*.sats DATS/*.dats
######
srclines_sats:: ; wc -l SATS/*.sats
srclines_dats:: ; wc -l DATS/*.dats
######

########################################################################

######
clean:: ; rm -f */SATS/*~
clean:: ; rm -f */DATS/*~
clean:: ; rm -f */HATS/*~
clean:: ; rm -f */DATS/CATS/CC/*~
clean:: ; rm -f */DATS/CATS/JS/*~
######
cleanall:: clean
######

