######
#
# For building xatsopt
#
######

AR=ar
CP=cp
MV=mv
CC=gcc
SED=sed
CPF=cp -f
MVF=mv -f
RMF=rm -f
WCL=wc -l

######
#
XATSOPT=$(XATSHOME)/bin/xatsopt
#
######
all::
######
all:: all_sats
######
all:: all_dats
######

######
#
# HX: processing SATS-files
#
######

######
all_sats:: \
xbasics_sats
xbasics_sats: ; $(XATSOPT) --static SATS/xbasics.sats
######
all_sats:: \
xstamp0_sats
xstamp0_sats: ; $(XATSOPT) --static SATS/xstamp0.sats
######
