#implfun
d4pat_fprint
(out, d4p0) =
let
#impltmp
g_print$out<>() = out
in//let
//
case+
d4p0.node() of
//
|D4Pint(tok) =>
print("D4Pint(",tok,")")
|D4Pbtf(sym) =>
print("D4Pbtf(",sym,")")
|D4Pchr(tok) =>
print("D4Pchr(",tok,")")
|D4Pflt(tok) =>
print("D4Pflt(",tok,")")
|D4Pstr(tok) =>
print("D4Pstr(",tok,")")
//
|
D4Perrck // HX: generated by [tread34]
(lvl1(*err-level*),dpat) => print("D4Perrck(",lvl1,";",dpat,")")
//
end (*let*) // end of [d4pat_fprint(out, d4p0)]

