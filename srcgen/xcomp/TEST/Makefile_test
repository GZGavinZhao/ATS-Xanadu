######
#
# For testing xatsopt
#
######
all::
######
MAKE=make
######
XATSOPT=\
./../../../bin/xatsopt
######
all:: \
prelude
prelude:: ; \
$(MAKE) XATSOPT=$(XATSOPT) -f Makefile_prelude > /dev/null
######
all:: \
xatslib
xatslib:: ; \
$(MAKE) XATSOPT=$(XATSOPT) -f Makefile_xatslib > /dev/null
######
###### end of [Makefile_test] ######
